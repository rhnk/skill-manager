{
  "name": "agent-manager",
  "version": "1.0.0",
  "description": "CLI tool to sync remote Git files, folders, repositories, and Gists to local skill folders",
  "main": "dist/index.js",
  "bin": {
    "agent-manager": "./bin/agent-manager.js",
    "am": "./bin/agent-manager.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "prepublishOnly": "npm run build",
    "test": "jest",
    "sync": "node bin/agent-manager.js sync",
    "sync:dry": "node bin/agent-manager.js sync --dry-run",
    "lint": "eslint src tests --ext .ts",
    "lint:fix": "eslint src tests --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\" \"{bin,}/*.js\" \"*.md\"",
    "format:check": "prettier --check \"src/**/*.ts\" \"tests/**/*.ts\" \"{bin,}/*.js\" \"*.md\"",
    "typecheck": "tsc --noEmit",
    "pre-commit": "npm run lint:fix && npm run format && npm run typecheck"
  },
  "keywords": [
    "cli",
    "skills",
    "git",
    "sync",
    "gist"
  ],
  "author": "",
  "dependencies": {
    "commander": "^12.0.0",
    "simple-git": "^3.22.0",
    "chalk": "^4.1.2",
    "ora": "^5.4.1",
    "fs-extra": "^11.2.0",
    "node-fetch": "^3.3.0",
    "zod": "^3.22.4",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^20.11.0",
    "@types/node-fetch": "^2.6.11",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.2",
    "prettier": "^3.1.1",
    "typescript": "^5.3.3",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.11",
    "ts-jest": "^29.1.2"
  }
}
